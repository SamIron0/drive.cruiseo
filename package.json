{
  "name": "chatbot-ui",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "chat": "supabase start && npm run db-types && npm run dev",
    "restart": "supabase stop && npm run chat",
    "update": "git pull origin main && npm run db-migrate && npm run db-types",
    "prepare": "husky install",
    "clean": "npm run lint:fix && npm run format:write",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "ANALYZE=true npm run build",
    "preview": "next build && next start",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"{app,lib,db,components,context,types}/**/*.{ts,tsx}\" --cache",
    "format:check": "prettier --check \"{app,lib,db,components,context,types}**/*.{ts,tsx}\" --cache",
    "db-reset": "supabase db reset && npm run db-types",
    "db-migrate": "supabase migration up && npm run db-types",
    "db-types": "supabase gen types typescript --local > supabase/types.ts",
    "db-pull": "supabase db remote commit",
    "db-push": "supabase db push",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@mui/x-date-pickers": "^6.20.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@stripe/react-stripe-js": "^2.7.1",
    "@stripe/stripe-js": "^3.4.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tabler/icons-react": "^3.5.0",
    "@types/google.maps": "^3.55.9",
    "@vercel/analytics": "^1.3.1",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "dayjs": "^1.11.11",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.379.0",
    "next": "^14.2.3",
    "next-i18n-router": "^5.4.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-date-range": "^2.0.1",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.2.1",
    "react-leaflet": "^4.2.1",
    "react-merge-refs": "^2.1.1",
    "react-spinners": "^0.13.8",
    "react-textarea-autosize": "^8.5.3",
    "resend": "^3.2.0",
    "sonner": "^1.4.41",
    "stripe": "^15.8.0",
    "vaul": "^0.9.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/common-tags": "^1.8.4",
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18",
    "@types/react-date-range": "^1.4.9",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "clsx": "^2.1.0",
    "eslint": "^8",
    "eslint-config-next": "^14.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "react-mobile-datepicker": "^4.0.2",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "uuid": "^9.0.1"
  }
}
